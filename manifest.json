{
    "manifest_version": 2,
    "name": "HangoutChatBot",
    "version": "0.0.1",

    "description": "Monitors open Google Hangouts chats and responds to various commands.",

    "content_scripts": [
        {
            "matches": ["*://talkgadget.google.com/*"],
            "js": ["lib/jquery-2.1.4.min.js", "bot.js"],
            "all_frames": true
        }
    ]
}
